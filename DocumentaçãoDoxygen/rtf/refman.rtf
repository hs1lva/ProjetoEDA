285     Fila* fila = criarFila();\par
286 \par
287     inserirElemFinalFila(fila, verticeOrigem); {\cf20 // Inserir v\'E9rtice origem na fila}\par
288     verticeOrigem->visitado = 1; {\cf20 // V\'E9rtice origem como visitado}\par
289     verticeOrigem->distancia = 0;\par
290     verticeOrigem->predecessor = -1;\par
291     {\cf18 int} encontrouDestino = 0;\par
292     \par
293     {\cf19 while} (!filaVazia(fila) && !encontrouDestino) \{ {\cf20 // Loop para v\'E9rtices na fila}\par
294         Vertice* verticeAtual = removerElemFila(fila);\par
295         Adjacente* adjacenteAtual = verticeAtual->adjacentes;\par
296 \par
297         {\cf19 while} (adjacenteAtual) \{ {\cf20 // Loop para todos os v\'E9rtices adjacentes do v\'E9rtice atual}\par
298             Vertice* adjacente = pesquisarVerticePorID(grafo, adjacenteAtual->idvertice);\par
299 \par
300             {\cf19 if} (!adjacente->visitado) \{\par
301                 \par
302                 adjacente->visitado = 1; {\cf20 // V\'E9rtice adjacente como visitado}\par
303                 inserirElemFinalFila(fila, adjacente); {\cf20 // Inserir v\'E9rtice adjacente na fila}\par
304 \par
305                 {\cf20 // Armazenar dados no predecessor}\par
306                 adjacente->predecessor = verticeAtual->idvertice;\par
307                 adjacente->distancia = verticeAtual->distancia + adjacenteAtual->distancia;\par
308 \par
309                 {\cf19 if} (adjacente->idvertice == destino) \{ {\cf20 // Parar se v\'E9rtice destino encontrado}\par
310                     encontrouDestino = 1;\par
311                     {\cf19 break};\par
312                 \}\par
313             \}\par
314             adjacenteAtual = adjacenteAtual->proximo;\par
315         \}\par
316     \}\par
317 \par
318     {\cf19 if} (verticeDestino->predecessor == -1) \{ {\cf20 // NULL se caminho do destino n\'E3o encontrado}\par
